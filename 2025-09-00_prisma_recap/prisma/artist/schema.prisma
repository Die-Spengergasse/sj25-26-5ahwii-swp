datasource db {
    provider = "sqlite"
    url      = "file:../../artist.db"
}

generator client {
    provider = "prisma-client"
    output   = "client"
    runtime  = "deno"
}

model Artist {
    id          String       @id @default(cuid())
    name        String
    instruments Instrument[]
    genres      Genre[]
}

model Instrument {
    id      String   @id @default(cuid())
    name    String
    artists Artist[]
}

model Genre {
    id      String   @id @default(cuid())
    name    String
    artists Artist[]
}
