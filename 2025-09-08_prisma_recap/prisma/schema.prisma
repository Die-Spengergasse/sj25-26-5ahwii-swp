datasource db {
    provider = "sqlite"
    url      = "file:../dev.db"
}

generator client {
    provider = "prisma-client-js"
}

model Question {
    id                String   @id @default(cuid())
    type              String
    difficulty        String
    category          String
    question          String
    incorrect_answers Answer[] @relation(name: "many")
    correct_answerId  String
}

model Answer {
    id                    String     @id @default(cuid())
    answer                String
    incorrect_in_question Question[] @relation(name: "many")
    questionId            String
}

